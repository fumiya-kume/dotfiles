Include ~/.colima/ssh_config

# 1Password SSH agent (configure keys in ~/.config/1Password/ssh/agent.toml).
Host *
  IdentitiesOnly yes

Match exec "test -S \"$HOME/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock\""
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"

Match exec "test -S \"$HOME/.1password/agent.sock\""
  IdentityAgent "~/.1password/agent.sock"
